<h1>Edit this gardener!</h1>

<%= form_for @gardener do |f| %>

    <div>
        <%= f.label :name %>
        <%= f.text_field :name %>
    </div>

    <div>
        <%= f.label :location %>
        <%= f.text_field :location %>
    </div>


    <%= f.submit "Submit these changes!" %>

<% end %>

<%= link_to "Back to the list!", gardeners_path %>
